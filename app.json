{
  "expo": {
    "name": "Dice Roll",
    "slug": "diceroll",
    "version": "1.0.0",
    // "orientation": "portrait",
    "orientation": "default",
    "icon": "./assets/images//bestpeers.png",
    "scheme": "diceroll",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#002C46"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bestpeers.diceroll",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#002C46"
      }
    },

    "android": {
      "package": "com.bestpeers.diceroll",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bestpeers.png",
        "backgroundColor": "#002C46"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#002C46"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#002C46",
          "imageWidth": 400,
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}


